<Page x:Class="AmbifluxAdmin.Default"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" Title="Ambiflux">
    <DockPanel Name="dockPanel1">
        <StackPanel Name="Top" Orientation="Vertical" DockPanel.Dock="Top" Height="120" ForceCursor="False" IsHitTestVisible="False">
                <Frame Margin="10" Name="frameTop" Source="Top.xaml" HorizontalAlignment="Left" />               
        </StackPanel>
        <StackPanel Name="stackPanel1" Orientation="Horizontal" DockPanel.Dock="Top">         
          <Frame Name="frameLeft" Source="Menu.xaml" Margin="10" MinWidth="200" MaxWidth="200" VerticalAlignment="Top" />
          <Frame Name="frameCenter" Margin="10" Source="Welcome.xaml" SandboxExternalContent="True" Width="800" VerticalAlignment="Top" />
          <Frame Name="frameRight" Source="Right.xaml" Margin="10,10,30,10" MinWidth="200" MaxWidth="200" VerticalAlignment="Top" />
        </StackPanel>
    </DockPanel>
</Page>
